<header class="content">
	<a id="logo" href="<%=path.prefix%>/<%=page.language%>/"><img src="<%=static%>/images/logo.png"
		width="242" height="78" alt=""></a>

  <% var pp = page.root.getContent('phone'); %>
  <% var aa = page.root.getContent('address'); %>

  <% if (pp || aa) { %>
    <div id="contactData">
     <% if (typeof pp !== "undefined") { %>
      <div class="contact tel"><%= pp.render() %></div>
     <% } %>
     <% if (typeof aa !== "undefined") { %>
      <div class="contact address"><%= aa.render() %></div>
     <% } %>
    </div>
  <% } %>


  <nav id="mainNav">
		<ul id="mainMenu">
      <% for (var i in page.root.children) {  
           var p = page.root.children[i]; 
           if (p.isVisible()) { %>
				    <li class="<%= (p == page) ? 'active' : '' %>">
					    <a href="<%=path.prefix%>/<%=p.url%>"><%= p.title %></a>
				    </li>
        <% } %>
      <% } %>
		</ul>
	</nav>

</header>